{
  "extends": [
    "development"
  ],
  "hints": {
    "typescript-config/strict": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-webkit-mask-image",
          "scrollbar-width"
        ]
      }
    ],
    "button-type": "off"
  }
}